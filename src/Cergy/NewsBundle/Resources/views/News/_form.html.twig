<form method="post">
    <!--print error from form-->
    <div>
        {{ form_label(form.title) }}
        {{ form_widget(form.title, { 'attr' : {'placeholder' : 'title here'}}) }}
        {{ form_errors(form.title) }}
    </div>

    <div>
        {{ form_label(form.content) }}
        {{ form_widget(form.content, { 'attr' : {'placeholder' : 'content here'}}) }}
        {{ form_errors(form.content) }}
    </div>

    {{ form_rest(form) }}
    <input type="submit"/>
</form>